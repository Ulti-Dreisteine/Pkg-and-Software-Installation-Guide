                    "fontStyle": "bold"
                }
            },
            {
                "scope": "string.quoted.multi.python",
                "settings": {
                    "foreground": "#ee1650d2",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "string.quoted.docstring.multi.python",
                "settings": {
                    "foreground": "#ee1650d2",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "string.quoted.single.python",
                "settings": {
                    "foreground": "#ee1650d2"
                }
            },
            {
                "scope": "constant",
                "settings": {
                    "foreground": "#ff8800"
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "foreground": "#000000"
                }
            },
            {
                "scope": "variable.language",
                "settings": {
                    "foreground": "#000d9bc5"
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#ff8800"
                }
            },
            {
                "scope": "variable.parameter.function.language.special.self.python",
                "settings": {
                    "foreground": "#ff8800",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#000000",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name",
                "settings": {
                    "foreground": "#000000",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.class",
                "settings": {
                    "foreground": "#d10000"
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#d10000",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#a9a9a9",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "punctuation.definition.comment.python",
                "settings": {
                    "foreground": "#a9a9a9"
                }
            },
            {
                "scope": "support.function.magic.python",
                "settings": {
                    "foreground": "#000000",
                }
            }
        ]
    },
